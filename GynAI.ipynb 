        "\n",
        "# UI Widgets (to be configured later)\n",
        "name_input = widgets.Text()\n",
        "gender_input = widgets.Dropdown()\n",
        "orientation_input = widgets.Text()\n",
        "symptom_input = widgets.Textarea()\n",
        "analyze_button = widgets.Button()\n",
        "output_area = widgets.Output()\n",
        "\n",
        "# Build interface based on selected language\n",
        "def build_ui(lang):\n",
        "    config = labels[lang]\n",
        "    name_input.description = config[\"name\"]\n",
        "    gender_input.options = config[\"gender_options\"]\n",
        "    gender_input.description = config[\"gender\"]\n",
        "    orientation_input.description = config[\"orientation\"]\n",
        "    symptom_input.description = config[\"symptoms\"]\n",
        "    symptom_input.placeholder = config[\"placeholder_symptoms\"]\n",
        "    analyze_button.description = config[\"button\"]\n",
        "\n",
        "    output_area.clear_output()\n",
        "    display(Markdown(f\"## {config['title']}\"))\n",
        "    display(name_input, gender_input, orientation_input, symptom_input, analyze_button, output_area)\n",
        "\n",
        "# Prompt generator\n",
        "def generate_prompt(name, gender, orientation, symptoms, lang):\n",
        "    intro = {\n",
        "        \"English\": f\"Name: {name}\\nGender identity: {gender}\\nSexual orientation: {orientation}\\nSymptoms: {symptoms}\",\n",
        "        \"Spanish\": f\"Nombre: {name}\\nIdentidad de g√©nero: {gender}\\nOrientaci√≥n sexual: {orientation}\\nS√≠ntomas: {symptoms}\",\n",
        "        \"French\": f\"Nom: {name}\\nIdentit√© de genre: {gender}\\nOrientation sexuelle: {orientation}\\nSympt√¥mes: {symptoms}\",\n",
        "        \"Chinese\": f\"ÂßìÂêç: {name}\\nÊÄßÂà´Ë∫´‰ªΩ: {gender}\\nÊÄßÂèñÂêë: {orientation}\\nÁóáÁä∂: {symptoms}\",\n",
        "        \"Vietnamese\": f\"T√™n: {name}\\nGi·ªõi t√≠nh: {gender}\\nXu h∆∞·ªõng t√≠nh d·ª•c: {orientation}\\nTri·ªáu ch·ª©ng: {symptoms}\"\n",
        "    }\n",
        "\n",
        "    task = {\n",
        "        \"English\": \"Analyze the symptoms considering the user's gender identity. Evaluate hormonal or cardiovascular conditions. Provide a non-diagnostic, inclusive recommendation.\",\n",
        "        \"Spanish\": \"Analiza los s√≠ntomas considerando la identidad de g√©nero. Eval√∫a posibles problemas hormonales o cardiovasculares. Da una recomendaci√≥n inclusiva sin diagnosticar.\",\n",
        "        \"French\": \"Analyse les sympt√¥mes en tenant compte de l'identit√© de genre. √âvalue les risques hormonaux ou cardiovasculaires. Donne une recommandation inclusive sans poser de diagnostic.\",\n",
        "        \"Chinese\": \"Ê†πÊçÆÊÄßÂà´Ë∫´‰ªΩÂàÜÊûêÁóáÁä∂„ÄÇËØÑ‰º∞ÊòØÂê¶ÂèØËÉΩÊòØÊøÄÁ¥†ÊàñÂøÉË°ÄÁÆ°ÈóÆÈ¢ò„ÄÇÊèê‰æõÈùûËØäÊñ≠ÊÄßÁöÑÂåÖÂÆπÊÄßÂª∫ËÆÆ„ÄÇ\",\n",
        "        \"Vietnamese\": \"Ph√¢n t√≠ch c√°c tri·ªáu ch·ª©ng theo gi·ªõi t√≠nh c·ªßa ng∆∞·ªùi d√πng. ƒê√°nh gi√° c√°c v·∫•n ƒë·ªÅ v·ªÅ n·ªôi ti·∫øt t·ªë ho·∫∑c tim m·∫°ch. Cung c·∫•p l·ªùi khuy√™n bao g·ªìm m√† kh√¥ng ƒë∆∞a ra ch·∫©n ƒëo√°n.\"\n",
        "    }\n",
        "\n",
        "    return f\"{intro[lang]}\\n\\n{task[lang]}\"\n",
        "\n",
        "# Button click handler\n",
        "def on_analyze_clicked(b):\n",
        "    with output_area:\n",
        "        clear_output()\n",
        "        lang = language_selector.value\n",
        "        prompt = generate_prompt(name_input.value, gender_input.value, orientation_input.value, symptom_input.value, lang)\n",
        "\n",
        "        response = co.generate(\n",
        "            model=\"command\",\n",
        "            prompt=prompt,\n",
        "            max_tokens=250,\n",
        "            temperature=0.7\n",
        "        )\n",
        "\n",
        "        title = labels[lang][\"result\"]\n",
        "        display(Markdown(f\"### ü©∫ {title}:\\n\\n{response.generations[0].text.strip()}\"))\n",
        "\n",
        "# Link analyze button\n",
        "analyze_button.on_click(on_analyze_clicked)\n",
        "\n",
        "# Language selection handler\n",
        "def on_language_selected(change):\n",
        "    clear_output()\n",
        "    build_ui(change.new)\n",
        "\n",
        "language_selector.observe(on_language_selected, names='value')\n",
        "\n",
        "# Initial display\n",
        "display(Markdown(\"## üåê Select your language\"))\n",
        "display(language_selector)\n"
